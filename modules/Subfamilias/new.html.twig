{% extends 'Subfamilias/index.html.twig' %}

{% block navegador %}
{% include "_global/comandosList.html.twig" with {'controller': values.controller, 'linkValue': values.linkBy.value} %}
{% endblock %}

{% block mantenimiento %}
<div class="FormManto">
    <form name="manto" action="" method="POST">
        <input name="controller" value="{{ values.controller }}" type="hidden" />
        <input name="action" id="action" value="new" type="hidden" />
        <input name="Subfamilias[IDFamilia]" value="{{values.linkBy.value}}" type="hidden" />
        <input name="Subfamilias[IDSubfamilia]" value="{{ values.datos.IDSubfamilia }}" type="hidden" />
        {% include '_global/comandosCreate.html.twig' %}

        <div class="Cuerpo">
	{% include '_global/FormErrores.html.twig' with {'errores': values.errores} %}
	{% include "Subfamilias/form.html.twig" with {'datos': values.datos} %}
        </div>
    </form>
</div>
{% endblock %}